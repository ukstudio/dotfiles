Config { font = "xft:Sans-9:bold"
       , bgColor = "#000000"
       , fgColor = "grey"
       , lowerOnStart = True
       , position = TopW L 100
       , commands = [  Run Cpu ["-L", "3", "-H", "50", "--normal", "green", "--high", "red"] 10
                    ,  Run Memory ["-t","Mem: <usedratio>%"] 10
                    ,  Run Swap [] 10
                    ,  Run Date "%Y/%m/%d %a %k:%M" "date" 10
                    ,  Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %cpu% %memory% %swap% }%StdinReader%{ <fc=#ee9a00>%date%</fc>"
}
